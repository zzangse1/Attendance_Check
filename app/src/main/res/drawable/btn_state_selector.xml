<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Disabled state -->
    <item android:drawable="@drawable/btn_clicked" android:state_enabled="false" />
    <!-- Default state (enabled and not pressed) -->
    <item android:drawable="@drawable/btn_unclicked" android:state_enabled="true" android:state_pressed="false" />
    <!-- Other states can be added as needed -->
</selector>
